<% car.forEach((car,i)=>{%>
<tr id="carTableRow<%=i%>" class="carTableRow" data-eco="<%= car.ecoType%>">
  <td>
    <input
      class="form-check-input"
      type="checkbox"
      id="inlineCheckbox<%=i%>"
      value="<%=car._id%>"
      onchange="selectRow(event,'<%=i%>')"
    />
  </td>
  <td id="car<%=i%>name"><%= car.name%></td>
  <td><%= car.type%></td>
  <td><%= car.fuel%></td>
  <td id="car<%=i%>price" data-price="<%= car.price %>">
    <%= Intl.NumberFormat('en-US', { style: 'currency', currency: 'EUR',
    }).format(car.price); %>
  </td>
  <td><%= car.ecoType%></td>
</tr>
<% }) %>
